{
  "description": "Quantity unit patterns",
  "units": {
    "g": 89,
    "г": 79,
    "ml": 59,
    "пранета": 6,
    "l": 8,
    "бр.": 26,
    "см": 43,
    "части": 4,
    "W": 6,
    "л": 9,
    "pcs": 40,
    "m": 1,
    "м": 1,
    "мл": 21,
    "Ah": 1,
    "А": 1,
    "GB": 2,
    "кг": 10,
    "гр": 1,
    "гнезда": 2,
    "kg": 1,
    "бр": 2,
    "чифт": 1
  },
  "regex_patterns": [
    {
      "pattern": "(\\d+(?:[.,]\\d+)?)\\s*(мл|ml)",
      "unit": "ml"
    },
    {
      "pattern": "(\\d+(?:[.,]\\d+)?)\\s*(л|l)(?:\\s|$)",
      "unit": "l"
    },
    {
      "pattern": "(\\d+(?:[.,]\\d+)?)\\s*(гр?|g)(?:\\s|$)",
      "unit": "g"
    },
    {
      "pattern": "(\\d+(?:[.,]\\d+)?)\\s*(кг|kg)",
      "unit": "kg"
    },
    {
      "pattern": "(\\d+)\\s*(бр)",
      "unit": "pcs"
    },
    {
      "pattern": "(\\d+)\\s*[xх]\\s*(\\d+(?:[.,]\\d+)?)\\s*(мл|ml|л|l|гр?|g|кг|kg)",
      "unit": "pack"
    }
  ]
}